{
    "type": "tuvssh",
    "path": "./data",
    "offset": 0,
    "num": 10,
    "pulsar": {
        "url": "pulsar://localhost:6650"
    },
    "logger": {
        "info": {
            "level": "Info",
            "writers": [{
                "type": "RotateFile",
                "rotateFileWriter": {
                    "filename": "log/run.log",
                    "maxAge": "24h",
                    "formatter": {
                        "type": "txt"
                    }
                }
            }]
        }
    }
}
